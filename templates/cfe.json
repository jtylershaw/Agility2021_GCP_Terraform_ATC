{
    "url": "/mgmt/shared/cloud-failover/declare",
    "method": "POST",
    "body": {
        
        "class": "Cloud_Failover",
        "environment": "gcp",
        "externalStorage": {
            "scopingTags": {
                "f5-cfe-failover-label": "student${student_id}"
            }
            
        },
        "failoverAddresses": {
            "enabled":true,
            "scopingTags": {
                "f5-cfe-failover-label": "student${student_id}"
            }
        },
        "controls": {
            "class": "Controls",
            "logLevel": "silly"
        }
    
}
}
